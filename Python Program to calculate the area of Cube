import math
def later_suface_area(edge_length):
    if (edge_length > 0):
        surface_area = 4*edge_length*edge_length
        print("later surface area is:",surface_area)
    else:
        print("it is not valid length")

def total_surface_area(edge_length):
    if (edge_length > 0):
        t_surface_area = 6*edge_length*edge_length
        print("total surface area is:", t_surface_area)
    else:
        print("it is not valid length")

edge_length =10;
later_suface_area(edge_length)
total_surface_area(edge_length)
